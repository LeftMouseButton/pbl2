{
  "disease_name": "Cervical cancer",
  "synonyms": [
    "Cervical carcinoma",
    "Cancer of the cervix"
  ],
  "summary": "A malignancy arising from the cervix, most commonly squamous cell carcinoma or adenocarcinoma, driven in the vast majority of cases by persistent infection with high‑risk human papillomavirus (HPV) leading to E6/E7‑mediated inactivation of p53 and Rb; preventable via HPV vaccination and screening (Pap/HPV tests).",
  "causes": [
    "Persistent high‑risk HPV infection (e.g., HPV‑16, HPV‑18)",
    "HPV E6-mediated TP53 degradation",
    "HPV E7-mediated RB1 inactivation",
    "Progression from cervical intraepithelial neoplasia (CIN)"
  ],
  "risk_factors": [
    "High-risk HPV infection",
    "Smoking",
    "Immunosuppression (e.g., HIV)",
    "Long-term oral contraceptive use",
    "High parity",
    "Early onset of sexual activity",
    "Multiple sexual partners",
    "In utero DES exposure"
  ],
  "symptoms": [
    "Postcoital bleeding",
    "Intermenstrual bleeding",
    "Abnormal vaginal discharge",
    "Pelvic pain",
    "Dyspareunia",
    "Back pain",
    "Leg swelling",
    "Hematuria",
    "Rectal bleeding",
    "Fatigue"
  ],
  "diagnosis": [
    "Pap smear (cytology)",
    "HPV DNA testing",
    "Colposcopy",
    "Cervical biopsy (punch, cone) / LEEP",
    "Endocervical curettage",
    "FIGO staging",
    "MRI/CT/PET for staging"
  ],
  "treatments": [
    "Conization/LEEP (for CIN and microinvasive)",
    "Radical hysterectomy with lymphadenectomy",
    "Fertility-sparing radical trachelectomy (early stage)",
    "External-beam radiotherapy (EBRT)",
    "Brachytherapy",
    "Concurrent chemoradiation (cisplatin-based)",
    "Chemotherapy (platinum-based)",
    "Bevacizumab (anti-VEGF)",
    "Immunotherapy (pembrolizumab; cemiplimab)",
    "Antibody–drug conjugate (tisotumab vedotin)"
  ],
  "related_genes": [
    "TP53",
    "RB1",
    "PIK3CA",
    "PTEN",
    "KRAS"
  ],
